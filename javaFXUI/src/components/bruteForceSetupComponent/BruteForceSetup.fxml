            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <Label fx:id="totalMissionNumLabel" prefHeight="31.0" prefWidth="64.0" text="##" GridPane.columnIndex="3" GridPane.rowIndex="3">
