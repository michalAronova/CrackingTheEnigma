<GridPane prefHeight="192.0" prefWidth="611.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.bruteForceSetupComponent.BruteForceSetupController">
  <columnConstraints>
      <ColumnConstraints fillWidth="false" maxWidth="147.0" minWidth="129.0" prefWidth="147.0" />
      <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="262.0" minWidth="194.0" prefWidth="246.0" />
    <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="182.0" minWidth="105.0" prefWidth="153.0" />
    <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="98.0" minWidth="27.0" prefWidth="66.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints vgrow="SOMETIMES" />
    <RowConstraints maxHeight="80.0" minHeight="41.0" prefHeight="41.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="110.0" minHeight="39.0" prefHeight="72.0" vgrow="SOMETIMES" />
    <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="ALWAYS" />
  </rowConstraints>
   <children>
      <Label prefHeight="41.0" prefWidth="578.0" text="Brute Force Setup" GridPane.columnSpan="4">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <Label text="Number of agents:" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
