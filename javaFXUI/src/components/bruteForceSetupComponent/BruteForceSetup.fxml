            <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
         </GridPane.margin>
      </TextField>
      <FlowPane GridPane.columnIndex="2" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin>
         <children>
            <Button fx:id="bruteForceSetButton" mnemonicParsing="false" onAction="#onSetButton" text="Set">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="bruteForceStartButton" mnemonicParsing="false" onAction="#onStartButton" text="Start!">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </children>
